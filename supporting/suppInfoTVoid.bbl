    \verb 10.1016/j.chroma.2012.07.029
    \endverb
    \field{issn}{1873-3778}
    \field{pages}{120\bibrangedash 126}
    \field{title}{Hydrodynamic radius determination with asymmetrical flow
  field-flow fractionation using decaying cross-flows. Part I. A theoretical
  approach.}
    \field{volume}{1253}
